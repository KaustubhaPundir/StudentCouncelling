pipeline{
    agent any

    environment {
        IMAGE_NAME = "kaustubhapundir/studentcouncillingBackend"
        TAG = "latest"
    }

    stages{
        stage('Building docker image'){
            steps{
                script{
                    docker.build("nmj:${TAG}","Backend/.")
                    echo "built successfull"
                }
            }
        }
    }
}
